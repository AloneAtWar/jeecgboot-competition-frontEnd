dateValue: function updateValue() {\n      this.$emit('change', this.disableChoice ? '?' : this.value_c);\n    },\n    preProcessProp: function preProcessProp(c) {\n      return replaceWeekName(c);\n    }\n  },\n  created: function created() {\n    this.DEFAULT_VALUE = '*'; // 0,7表示周日 1表示周一\n\n    this.minValue = 1;\n    this.maxValue = 7;\n    this.valueRange.start = 1;\n    this.valueRange.end = 7;\n    this.valueLoop.start = 2;\n    this.valueLoop.interval = 1;\n    this.parseProp(this.prop);\n  }\n};",{"version":3,"sources":["week.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAiDA,OAAA,KAAA,MAAA,SAAA;AACA,SAAA,eAAA,EAAA,WAAA,QAAA,YAAA;AAEA,IAAA,QAAA,GAAA;AACA,QAAA,CADA;AAEA,QAAA,CAFA;AAGA,QAAA,CAHA;AAIA,QAAA,CAJA;AAKA,QAAA,CALA;AAMA,QAAA,CANA;AAOA;AACA,QAAA;AARA,CAAA;AAWA,eAAA;AACA,EAAA,IAAA,EAAA,MADA;AAEA,EAAA,MAAA,EAAA,CAAA,KAAA,CAFA;AAGA,EAAA,KAAA,EAAA;AACA,IAAA,GAAA,EAAA;AACA,MAAA,IAAA,EAAA,MADA;AAEA,MAAA,OAAA,EAAA;AAFA;AADA,GAHA;AASA,EAAA,IATA,kBASA;AACA,WAAA;AACA,MAAA,QAAA,EAAA,QADA;AAEA,MAAA,WAAA,EAAA;AAFA,KAAA;AAIA,GAdA;AAeA,EAAA,QAAA,EAAA;AACA,IAAA,aADA,2BACA;AACA,aAAA,KAAA,GAAA,IAAA,KAAA,GAAA,KAAA,GAAA,IAAA,KAAA,QAAA;AACA;AAHA,GAfA;AAoBA,EAAA,KAAA,EAAA;AACA,IAAA,OADA,mBACA,MADA,EACA,MADA,EACA;AACA;AACA,WAAA,WAAA;AACA,KAJA;AAKA,IAAA,GALA,eAKA,MALA,EAKA;AACA;AACA,WAAA,WAAA;AACA;AARA,GApBA;AA8BA,EAAA,OAAA,EAAA;AACA,IAAA,WADA,yBACA;AACA,WAAA,KAAA,CAAA,QAAA,EAAA,KAAA,aAAA,GAAA,GAAA,GAAA,KAAA,OAAA;AACA,KAHA;AAIA,IAAA,cAJA,0BAIA,CAJA,EAIA;AACA,aAAA,eAAA,CAAA,CAAA,CAAA;AACA;AANA,GA9BA;AAsCA,EAAA,OAtCA,qBAsCA;AACA,SAAA,aAAA,GAAA,GAAA,CADA,CAEA;;AACA,SAAA,QAAA,GAAA,CAAA;AACA,SAAA,QAAA,GAAA,CAAA;AACA,SAAA,UAAA,CAAA,KAAA,GAAA,CAAA;AACA,SAAA,UAAA,CAAA,GAAA,GAAA,CAAA;AACA,SAAA,SAAA,CAAA,KAAA,GAAA,CAAA;AACA,SAAA,SAAA,CAAA,QAAA,GAAA,CAAA;AACA,SAAA,SAAA,CAAA,KAAA,IAAA;AACA;AAhDA,CAAA","sourcesContent":["<template>\r\n  <div class=\"config-list week\">\r\n    <a-radio-group v-model=\"type\">\r\n      <div class=\"item\">\r\n        <a-radio value=\"TYPE_NOT_SET\" class=\"choice\" :disabled=\"disableChoice\">不设置</a-radio>\r\n        <span class=\"tip-info\">日和周只能设置其中之一</span>\r\n      </div>\r\n      <div class=\"item\">\r\n        <a-radio value=\"TYPE_RANGE\" class=\"choice\" :disabled=\"disableChoice\">区间</a-radio>\r\n        从\r\n        <a-select v-model=\"valueRange.start\" class=\"w80\" :disabled=\"type!==TYPE_RANGE || disableChoice\">\r\n          <template v-for=\"(v, k) of WEEK_MAP\">\r\n            <a-select-option :value=\"v\">{{k}}</a-select-option>\r\n          </template>\r\n        </a-select>\r\n        至\r\n        <a-select v-model=\"valueRange.end\" class=\"w80\" :disabled=\"type!==TYPE_RANGE || disableChoice\">\r\n          <template v-for=\"(v, k) of WEEK_MAP\">\r\n            <a-select-option :value=\"v\">{{k}}</a-select-option>\r\n          </template>\r\n        </a-select>\r\n      </div>\r\n      <div class=\"item\">\r\n        <a-radio value=\"TYPE_LOOP\" class=\"choice\" :disabled=\"disableChoice\">循环</a-radio>\r\n        从\r\n        <a-select v-model=\"valueLoop.start\" class=\"w80\" :disabled=\"type!==TYPE_LOOP || disableChoice\">\r\n          <template v-for=\"(v, k) of WEEK_MAP\">\r\n            <a-select-option :value=\"v\">{{k}}</a-select-option>\r\n          </template>\r\n        </a-select>\r\n        开始，间隔\r\n        <a-input-number :disabled=\"type!==TYPE_LOOP || disableChoice\" :max=\"maxValue\" :min=\"minValue\" :precision=\"0\" class=\"w60\" v-model=\"valueLoop.interval\"/>\r\n        天\r\n      </div>\r\n      <div class=\"item\">\r\n        <a-radio value=\"TYPE_SPECIFY\" class=\"choice\" :disabled=\"disableChoice\">指定</a-radio>\r\n        <div class=\"list\">\r\n          <a-checkbox-group v-model=\"valueList\">\r\n            <template v-for=\"(v,k) in WEEK_MAP\">\r\n              <a-checkbox class=\"list-check-item\" :key=\"`key-${v}`\" :value=\"v\" :disabled=\"type!==TYPE_SPECIFY || disabled\">{{k}}</a-checkbox>\r\n            </template>\r\n          </a-checkbox-group>\r\n        </div>\r\n      </div>\r\n    </a-radio-group>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport mixin from './mixin'\r\nimport { replaceWeekName, WEEK_MAP_EN } from './const.js'\r\n\r\nconst WEEK_MAP = {\r\n  '周一': 2,\r\n  '周二': 3,\r\n  '周三': 4,\r\n  '周四': 5,\r\n  '周五': 6,\r\n  '周六': 7,\r\n  // 按照国人习惯，将周日放到每周的最后一天\r\n  '周日': 1,\r\n}\r\n\r\nexport default {\r\n  name: 'week',\r\n  mixins: [mixin],\r\n  props: {\r\n    day: {\r\n      type: String,\r\n      default: '*'\r\n    }\r\n  },\r\n  data() {\r\n    return {\r\n      WEEK_MAP,\r\n      WEEK_MAP_EN\r\n    }\r\n  },\r\n  computed: {\r\n    disableChoice() {\r\n      return (this.day && this.day !== '?') || this.disabled\r\n    }\r\n  },\r\n  watch: {\r\n    value_c(newVal, oldVal) {\r\n      // 如果设置日，那么星期就直接不设置\r\n      this.updateValue()\r\n    },\r\n    day(newVal) {\r\n      // console.info('new day: ' + newVal)\r\n      this.updateValue()\r\n    }\r\n  },\r\n  methods: {\r\n    updateValue() {\r\n      this.$emit('change', this.disableChoice ? '?' : this.value_c)\r\n    },\r\n    preProcessProp(c) {\r\n      return replaceWeekName(c)\r\n    }\r\n  },\r\n  created() {\r\n    this.DEFAULT_VALUE = '*'\r\n    // 0,7表示周日 1表示周一\r\n    this.minValue = 1\r\n    this.maxValue = 7\r\n    this.valueRange.start = 1\r\n    this.valueRange.end = 7\r\n    this.valueLoop.start = 2\r\n    this.valueLoop.interval = 1\r\n    this.parseProp(this.prop)\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"less\" scoped>\r\n  @import \"mixin.less\";\r\n</style>\r\n"],"sourceRoot":"src/components/jeecg/JEasyCron/tabs"}]}