ss=\"table-page-search-wrapper\">\r\n      <a-form layout=\"inline\" @keyup.enter.native=\"searchQuery\">\r\n        <a-row :gutter=\"24\">\r\n          <a-col :xl=\"6\" :lg=\"7\" :md=\"8\" :sm=\"24\">\r\n            <a-form-item label=\"用户\">\r\n              <j-dict-select-tag placeholder=\"请选择用户\" v-model=\"queryParam.userId\" dictCode=\"sys_user,username,id\"/>\r\n            </a-form-item>\r\n          </a-col>\r\n          <a-col :xl=\"6\" :lg=\"7\" :md=\"8\" :sm=\"24\">\r\n            <a-form-item label=\"手机号\">\r\n              <a-input placeholder=\"请输入手机号\" v-model=\"queryParam.phone\"></a-input>\r\n            </a-form-item>\r\n          </a-col>\r\n          <template v-if=\"toggleSearchStatus\">\r\n            <a-col :xl=\"6\" :lg=\"7\" :md=\"8\" :sm=\"24\">\r\n              <a-form-item label=\"报名大赛\">\r\n                <j-dict-select-tag placeholder=\"请选择报名大赛\" v-model=\"queryParam.competitionId\" dictCode=\"competition,name,id\"/>\r\n              </a-form-item>\r\n            </a-col>\r\n            <a-col :xl=\"6\" :lg=\"7\" :md=\"8\" :sm=\"24\">\r\n              <a-form-item label=\"学校\">\r\n                <a-input placeholder=\"请输入学校\" v-model=\"queryParam.school\"></a-input>\r\n              </a-form-item>\r\n            </a-col>\r\n            <a-col :xl=\"6\" :lg=\"7\" :md=\"8\" :sm=\"24\">\r\n              <a-form-item label=\"学历\">\r\n                <a-input placeholder=\"请输入学历\" v-model=\"queryParam.education\"></a-input>\r\n              </a-form-item>\r\n            </a-col>\r\n            <a-col :xl=\"6\" :lg=\"7\" :md=\"8\" :sm=\"24\">\r\n              <a-form-item label=\"专业\">\r\n                <a-input placeholder=\"请输入专业\" v-model=\"queryParam.professional\"></a-input>\r\n              </a-form-item>\r\n            </a-col>\r\n          </template>\r\n          <a-col :xl=\"6\" :lg=\"7\" :md=\"8\" :sm=\"24\">\r\n            <span style=\"float: left;overflow: hidden;\" class=\"table-page-search-submitButtons\">\r\n              <a-button type=\"primary\" @click=\"searchQuery\" icon=\"search\">查询</a-button>\r\n              <a-button type=\"primary\" @click=\"searchReset\" icon=\"reload\" style=\"margin-left: 8px\">重置</a-button>\r\n              <a @click=\"handleToggleSearch\" style=\"margin-left: 8px\">\r\n                {{ toggleSearchStatus ? '收起' : '展开' }}\r\n                <a-icon :type=\"toggleSearchStatus ? 'up' : 'down'\"/>\r\n              </a>\r\n            </span>\r\n          </a-col>\r\n        </a-row>\r\n      </a-form>\r\n    </div>\r\n    <!-- 查询区域-END -->\r\n\r\n    <!-- 操作按钮区域 -->\r\n    <div class=\"table-operator\">\r\n      <a-button @click=\"handleAdd\" type=\"primary\" icon=\"plus\">新增</a-button>\r\n      <a-button type=\"primary\" icon=\"download\" @click=\"handleExportXls('大赛申请表')\">导出</a-button>\r\n      <a-upload name=\"file\" :showUploadList=\"false\" :multiple=\"false\" :headers=\"tokenHeader\" :action=\"importExcelUrl\" @change=\"handleImportExcel\">\r\n        <a-button type=\"primary\" icon=\"import\">导入</a-button>\r\n      </a-upload>\r\n      <!-- 高级查询区域 -->\r\n      <j-super-query :fieldList=\"superFieldList\" ref=\"superQueryModal\" @handleSuperQuery=\"handleSuperQuery\"></j-super-query>\r\n      <a-dropdown v-if=\"selectedRowKeys.length > 0\">\r\n        <a-menu slot=\"overlay\">\r\n          <a-menu-item key=\"1\" @click=\"batchDel\"><a-icon type=\"delete\"/>删除</a-menu-item>\r\n        </a-menu>\r\n        <a-button style=\"margin-left: 8px\"> 批量操作 <a-icon type=\"down\" /></a-button>\r\n      </a-dropdown>\r\n    </div>\r\n\r\n    <!-- table区域-begin -->\r\n    <div>\r\n      <div class=\"ant-alert ant-alert-info\" style=\"margin-bottom: 16px;\">\r\n        <i class=\"anticon anticon-info-circle ant-alert-icon\"></i> 已选择 <a style=\"font-weight: 600\">{{ selectedRowKeys.length }}</a>项\r\n        <a style=\"margin-left: 24px\" @click=\"onClearSelected\">清空</a>\r\n      </div>\r\n\r\n      <a-table\r\n        ref=\"table\"\r\n        size=\"middle\"\r\n        :scroll=\"{x:true}\"\r\n        bordered\r\n        rowKey=\"id\"\r\n        :columns=\"columns\"\r\n        :dataSource=\"dataSource\"\r\n        :pagination=\"ipagination\"\r\n        :loading=\"loading\"\r\n        :rowSelection=\"{selectedRowKeys: selectedRowKeys, onChange: onSelectChange}\"\r\n        class=\"j-table-force-nowrap\"\r\n        @change=\"handleTableChange\">\r\n\r\n        <template slot=\"htmlSlot\" slot-scope=\"text\">\r\n          <div v-html=\"text\"></div>\r\n        </template>\r\n        <template slot=\"imgSlot\" slot-scope=\"text,record\">\r\n          <span v-if=\"!text\" style=\"font-size: 12px;font-style: italic;\">无图片</span>\r\n          <img v-else :src=\"getImgView(text)\" :preview=\"record.id\" height=\"25px\" alt=\"\" style=\"max-width:80px;font-size: 12px;font-style: italic;\"/>\r\n        </template>\r\n        <template slot=\"fileSlot\" slot-scope=\"text\">\r\n          <span v-if=\"!text\" style=\"font-size: 12px;font-style: italic;\">无文件</span>\r\n          <a-button\r\n            v-else\r\n            :ghost=\"true\"\r\n            type=\"primary\"\r\n            icon=\"download\"\r\n            size=\"small\"\r\n            @click=\"downloadFile(text)\">\r\n            下载\r\n          </a-button>\r\n        </template>\r\n\r\n        <span slot=\"action\" slot-scope=\"text, record\">\r\n          <a @click=\"handleEdit(record)\">编辑</a>\r\n\r\n          <a-divider type=\"vertical\" />\r\n          <a-dropdown>\r\n            <a class=\"ant-dropdown-link\">更多 <a-icon type=\"down\" /></a>\r\n            <a-menu slot=\"overlay\">\r\n              <a-menu-item>\r\n                <a @click=\"handleDetail(record)\">详情</a>\r\n              </a-menu-item>\r\n              <a-menu-item>\r\n                <a-popconfirm title=\"确定删除吗?\" @confirm=\"() => handleDelete(record.id)\">\r\n                  <a>删除</a>\r\n                </a-popconfirm>\r\n              </a-menu-item>\r\n            </a-menu>\r\n          </a-dropdown>\r\n        </span>\r\n\r\n      </a-table>\r\n    </div>\r\n\r\n    <competition-enroll-modal ref=\"modalForm\" @ok=\"modalFormOk\"></competition-enroll-modal>\r\n  </a-card>\r\n</template>\r\n\r\n<script>\r\n\r\n  import '@/assets/less/TableExpand.less'\r\n  import { mixinDevice } from '@/utils/mixin'\r\n  import { JeecgListMixin } from '@/mixins/JeecgListMixin'\r\n  import CompetitionEnrollModal from './modules/CompetitionEnrollModal'\r\n  import {filterMultiDictText} from '@/components/dict/JDictSelectUtil'\r\n\r\n  export default {\r\n    name: 'CompetitionEnrollList',\r\n    mixins:[JeecgListMixin, mixinDevice],\r\n    components: {\r\n      CompetitionEnrollModal\r\n    },\r\n    data () {\r\n      return {\r\n        description: '大赛申请表管理页面',\r\n        // 表头\r\n        columns: [\r\n          {\r\n            title: '#',\r\n            dataIndex: '',\r\n            key:'rowIndex',\r\n            width:60,\r\n            align:\"center\",\r\n            customRender:function (t,r,index) {\r\n              return parseInt(index)+1;\r\n            }\r\n          },\r\n          {\r\n            title:'用户',\r\n            align:\"center\",\r\n            dataIndex: 'userId_dictText'\r\n          },\r\n          {\r\n            title:'手机号',\r\n            align:\"center\",\r\n            dataIndex: 'phone'\r\n          },\r\n          {\r\n            title:'报名大赛',\r\n            align:\"center\",\r\n            dataIndex: 'competitionId_dictText'\r\n          },\r\n          {\r\n            title:'学校',\r\n            align:\"center\",\r\n            dataIndex: 'school'\r\n          },\r\n          {\r\n            title:'学历',\r\n            align:\"center\",\r\n            dataIndex: 'education'\r\n          },\r\n          {\r\n            title:'专业',\r\n            align:\"center\",\r\n            dataIndex: 'professional'\r\n          },\r\n          {\r\n            title:'状态',\r\n            align:\"center\",\r\n            dataIndex: 'state_dictText'\r\n          },\r\n          {\r\n            title:'理由',\r\n            align:\"center\",\r\n            dataIndex: 'reason'\r\n          },\r\n          {\r\n            title: '操作',\r\n            dataIndex: 'action',\r\n            align:\"center\",\r\n            fixed:\"right\",\r\n            width:147,\r\n            scopedSlots: { customRender: 'action' }\r\n          }\r\n        ],\r\n        url: {\r\n          list: \"/competition/competitionEnroll/list\",\r\n          delete: \"/competition/competitionEnroll/delete\",\r\n          deleteBatch: \"/competition/competitionEnroll/deleteBatch\",\r\n          exportXlsUrl: \"/competition/competitionEnroll/exportXls\",\r\n          importExcelUrl: \"competition/competitionEnroll/importExcel\",\r\n          \r\n        },\r\n        dictOptions:{},\r\n        superFieldList:[],\r\n      }\r\n    },\r\n    created() {\r\n    this.getSuperFieldList();\r\n    },\r\n    computed: {\r\n      importExcelUrl: function(){\r\n        return `${window._CONFIG['domianURL']}/${this.url.importExcelUrl}`;\r\n      },\r\n    },\r\n    methods: {\r\n      initDictConfig(){\r\n      },\r\n      getSuperFieldList(){\r\n        let fieldList=[];\r\n        fieldList.push({type:'string',value:'userId',text:'用户',dictCode:\"sys_user,username,id\"})\r\n        fieldList.push({type:'string',value:'phone',text:'手机号',dictCode:''})\r\n        fieldList.push({type:'string',value:'competitionId',text:'报名大赛',dictCode:\"competition,name,id\"})\r\n        fieldList.push({type:'string',value:'school',text:'学校',dictCode:''})\r\n        fieldList.push({type:'string',value:'education',text:'学历',dictCode:''})\r\n        fieldList.push({type:'string',value:'professional',text:'专业',dictCode:''})\r\n        fieldList.push({type:'int',value:'state',text:'状态',dictCode:'competition_enroll_state'})\r\n        fieldList.push({type:'Text',value:'reason',text:'理由',dictCode:''})\r\n        this.superFieldList = fieldList\r\n      }\r\n    }\r\n  }\r\n</script>\r\n<style scoped>\r\n  @import '~@assets/less/common.less';\r\n</style>"]}]}